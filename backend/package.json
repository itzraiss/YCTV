  "description": "Backend da plataforma de streaming brasileira StreamVault - Sistema tipo Acteia",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "test": "jest --coverage",
    "test:watch": "jest --watch",
    "lint": "eslint src/",
    "lint:fix": "eslint src/ --fix",
    "sync:catalog": "node src/jobs/syncCatalog.js",
    "sync:trending": "node src/jobs/updateTrending.js",
